<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context="com.example.harshkumarbhartiya.tictactoe.Main2Activity">

    <Button
        android:id="@+id/button24"
        android:layout_width="118dp"
        android:layout_height="122dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:onClick="on6"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/button23"
        app:layout_constraintLeft_toRightOf="@+id/button15"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button16"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button23"
        android:layout_width="127dp"
        android:layout_height="122dp"
        android:background="@android:color/holo_blue_light"
        android:backgroundTint="@color/colorAccent"
        android:onClick="on9"
        android:visibility="visible"
        tools:layout_editor_absoluteY="295dp"
        tools:layout_editor_absoluteX="280dp" />

    <Button
        android:id="@+id/button22"
        android:layout_width="117dp"
        android:layout_height="121dp"
        android:layout_marginBottom="-34dp"
        android:layout_marginLeft="13dp"
        android:onClick="on2"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintLeft_toRightOf="@+id/textView" />

    <Button
        android:id="@+id/button21"
        android:layout_width="117dp"
        android:layout_height="121dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="8dp"
        android:onClick="on8"
        android:visibility="visible"
        app:layout_constraintLeft_toRightOf="@+id/button20"
        app:layout_constraintTop_toBottomOf="@+id/button15" />

    <Button
        android:id="@+id/button20"
        android:layout_width="117dp"
        android:layout_height="121dp"
        android:layout_marginLeft="-8dp"
        android:layout_marginTop="7dp"
        android:onClick="on7"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button14" />

    <Button
        android:id="@+id/button19"
        android:layout_width="117dp"
        android:layout_height="121dp"
        android:onClick="on1"
        android:visibility="visible"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="27dp" />

    <Button
        android:id="@+id/button16"
        android:layout_width="125dp"
        android:layout_height="120dp"
        android:layout_marginBottom="-120dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:onClick="on3"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintLeft_toRightOf="@+id/button22"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button14"
        android:layout_width="117dp"
        android:layout_height="121dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="25dp"
        android:onClick="on4"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/button15"
        android:layout_width="120dp"
        android:layout_height="123dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="25dp"
        android:onClick="on5"
        android:visibility="visible"
        app:layout_constraintLeft_toRightOf="@+id/button14"
        app:layout_constraintTop_toBottomOf="@+id/button22" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="114dp"
        android:layout_height="117dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/holo_blue_bright"
        android:textAllCaps="false"
        android:textSize="36sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/button19"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintRight_toRightOf="parent" />


</android.support.constraint.ConstraintLayout>
